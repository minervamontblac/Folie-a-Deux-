#define _CRT_SECURE_NO_WARNINGS 1

#include<stdio.h>
#include<string.h>
int main()
{
	printf("c:\\test\\32\\test.c");/*  \\表示反义单斜杠*/
	printf("%C\n",'\'');
	printf("%s\n","\"");
	printf("%d\n",strlen("c:\test\32\test.c"));/* \32表示作为8进制代表的10进制数的ASCII码值*/
	printf("%c\n",'\x61');/* \x61表示作为16进制代表的10进制数的ASCII码值*/
	return 0;
}
